{
  "name": "@cs/webpages-packer",
  "author": "",
  "description": "物智云平台开发构建工具",
  "license": "MIT",
  "type": "module",
  "version": "1.0.999",
  "module": "./dist/webpages-server-cli.js",
  "files": [
    "dist",
    "package.json"
  ],
  "bin": {
    "webpages-packer": "dist/webpages-packer-cli.js",
    "webpages-server": "dist/webpages-server-cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/webpages-packer-cli.js"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prebuild": "tsc -p src/tsconfig.json",
    "prepublishOnly": "npm run build",
    "lint": "eslint",
    "dev": "tsx watch src/bin/webpages-server-cli.ts start --env deveploment",
    "build2": "tsdown --clean",
    "build": "tsdown --clean --dts",
    "watch": "tsdown --watch"
  },
  "dependencies": {
    "tree-kill": "1.2.2",
    "deepmerge": "^4.3.1",
    "param-case": "3.0.4",
    "jiti": "^2.4.2",
    "webpack-merge": "^6.0.1",
    "@rspack/cli": "1.3.9",
    "@rspack/core": "1.3.10",
    "@rsbuild/core": "1.3.18",
    "@stricli/core": "^1.2.0",
    "@stricli/auto-complete": "^1.2.0",
    "webpack-node-externals": "3.0.0",
    "lodash.get": "4.4.2",
    "rslog": "^1.2.3",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "catalog:dev",
    "tinyglobby": "catalog:dev",
    "tsdown": "catalog:dev",
    "tsx": "catalog:dev",
    "typescript": "catalog:dev",
    "vitest": "catalog:dev",
    "@types/webpack-node-externals": "^3.0.4",
    "@types/lodash.get": "^4.4.2",
    "@types/deepmerge": "^2.2.3",
    "@types/fs-extra": "^11.0.4",
    "fs-extra": "^11.3.0"
  }
}
